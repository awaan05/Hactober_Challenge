import java.util.*;
public class Binet {
 
    static int fib(int n)
    {
        double phi = (1 + Math.sqrt(5)) / 2;
        return (int)Math.round(Math.pow(phi, n)
                               / Math.sqrt(5));
    }
    public static void main(String[] args)
    {
        int n = 9;
        System.out.println(fib(n));
    }
};
